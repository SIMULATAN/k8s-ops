prometheus.operator.servicemonitors "servicemonitors" {
  clustering {
    enabled = true
  }

  forward_to = [prometheus.relabel.target.receiver]
}

prometheus.operator.podmonitors "podmonitors" {
  clustering {
    enabled = true
  }

  forward_to = [prometheus.relabel.target.receiver]
}

prometheus.operator.scrapeconfigs "scrapeconfigs" {
  clustering {
    enabled = true
  }

  forward_to = [prometheus.relabel.target.receiver]
}
