{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prHourlyLimit": 5,
  "separateMultipleMajor": true,
  "additionalBranchPrefix": "{{parentDir}}/",
  "semanticCommitScope": "{{parentDir}}",
  "semanticCommits": "enabled",
  "commitMessageLowerCase": "never",
  "kubernetes": {
    "fileMatch": ["\\.yml$", "\\.yaml$"]
  }
}
